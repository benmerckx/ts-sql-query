{
    "name": "ts-sql-query",
    "version": "0.17.0",
    "description": "Type-safe SQL query builder like QueryDSL or JOOQ in Java or Linq in .Net for TypeScript with MariaDB, MySql, Oracle, PostgreSql, Sqlite and SqlServer support.",
    "license": "MIT",
    "author": "juanluispaz",
    "readmeFilename": "README.md",
    "keywords": [
        "jooq",
        "querydsl",
        "typescript",
        "mariadb",
        "mysql",
        "oracle",
        "sqlite",
        "sqlserver",
        "sql-server",
        "sql server",
        "postgre sql",
        "postgresql",
        "typesql",
        "type-sql",
        "type sql",
        "type-safe sql",
        "type safe sql",
        "typesafe sql",
        "sql",
        "query builder",
        "query",
        "orm",
        "linq"
    ],
    "scripts": {
        "example": "npm run build && node dist/exampleTests.js",
        "all-examples": "npm run build && sh ./scripts/run-all-examples.sh",
        "build": "rm -rf dist/*; tsc",
        "dist": "npm run build && cp -t dist LICENSE.md package.json README.md && cd dist && rm exampleTests* && rm -Rf examples && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/juanluispaz/ts-sql-query.git"
    },
    "bugs": {
        "url": "https://github.com/juanluispaz/ts-sql-query/issues"
    },
    "devDependencies": {
        "typescript": "^3.3.3"
    },
    "peerDependencies": {
        "@types/any-db": "^2.1.30",
        "@types/any-db-transaction": "^2.2.30",
        "@types/better-sqlite3": "^5.4.0",
        "@types/geojson": "^7946.0.7",
        "@types/mssql": "^4.0.15",
        "@types/mysql": "^2.15.6",
        "@types/mysql2": "github:types/mysql2",
        "@types/oracledb": "^3.1.3",
        "@types/pg": "^7.4.14",
        "@types/sqlite3": "^3.1.5",
        "@types/tedious": "^4.0.0",
        "@types/tedious-connection-pool": "^1.0.2",
        "loopback-datasource-juggler": "^4.19.2",
        "mariadb": "^2.0.5",
        "sqlite": "^4.0.14"
    },
    "dependencies": {
        "chained-error": "^1.0.0",
        "ts-extended-types": "^0.10.0"
    }
}
